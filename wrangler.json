{
  "name": "mcp-client",
  "main": "src/server.ts",
  "compatibility_date": "2025-03-14",
  "compatibility_flags": ["nodejs_compat"],
  "ai": {
    "binding": "AI",
    "remote": true
  },
  "vars": {
    "HOST": "mcp.elula.cloud",
    "MCP_PORTAL_URL": "https://elulaportal.elula.workers.dev/mcp",
    "GATEWAY_ID":"paraat-001"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "MyAgent",
        "class_name": "MyAgent"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["MyAgent"]
    }
  ],
  "observability": {
    "logs": {
      "enabled": true
    }
  }
}
